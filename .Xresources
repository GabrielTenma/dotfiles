! X colors.
! Generated by 'wal'
*foreground:        #7A7A7A
*background:        #fefefe
*.foreground:       #7A7A7A
*.background:       #fefefe
emacs*foreground:   #6D6168
emacs*background:   #fefefe
URxvt*foreground:   #7A7A7A
XTerm*foreground:   #7A7A7A
UXTerm*foreground:  #7A7A7A
URxvt*background:   [100]#fefefe
XTerm*background:   #fefefe
UXTerm*background:  #fefefe
URxvt*cursorColor:  #EEEEEE
XTerm*cursorColor:  #EEEEEE
UXTerm*cursorColor: #EEEEEE
URxvt*borderColor:  [100]#fefefe
URxvt.scrollBar: false
URxvt.internalBorder:  17
URxvt.geometry: 72x16
URxvt.font: xft:Iosevka Medium:size=9.5

! clipboard
! update clipboard whenever primary gets updated
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped


! Colors 0-15.
*.color0: #fefefe
*color0:  #fefefe
*.color1: #D3B2B0
*color1:  #D3B2B0
*.color2: #E3B0AA
*color2:  #E3B0AA
*.color3: #D8C3BC
*color3:  #D8C3BC
*.color4: #E4C4BC
*color4:  #E4C4BC
*.color5: #BCB1CE
*color5:  #BCB1CE
*.color6: #CBBACD
*color6:  #CBBACD
*.color7: #6D6168
*color7:  #6D6168
*.color8: #989897
*color8:  #989897
*.color9: #E9A39E
*color9:  #E9A39E
*.color10: #E3B0AA
*color10:  #E3B0AA
*.color11: #D8C3BC
*color11:  #D8C3BC
*.color12: #ED9795
*color12:  #ED9795
*.color13: #BCB1CE
*color13:  #BCB1CE
*.color14: #CBBACD
*color14:  #CBBACD
*.color15: #6D6168
*color15:  #6D6168

! Black color that will not be affected by bold highlighting.
*.color66: #fefefe
*color66:  #fefefe

! Xclock colors.
XClock*foreground: #6D6168
XClock*background: #fefefe
XClock*majorColor:  rgba:6d/61/68/ff
XClock*minorColor:  rgba:6d/61/68/ff
XClock*hourColor:   rgba:6d/61/68/ff
XClock*minuteColor: rgba:6d/61/68/ff
XClock*secondColor: rgba:6d/61/68/ff

! Set depth to make transparency work.
!URxvt*depth: 32




!! Yeah, I am one of those, who use these keys in Vim :-(
urxvt.keysym.Home:          \033[1~
urxvt.keysym.End:           \033[4~
urxvt.keysym.Control-Up:    \033[1;5A
urxvt.keysym.Control-Down:  \033[1;5B
urxvt.keysym.Control-Left:  \033[1;5D
urxvt.keysym.Control-Right: \033[1;5C

!! Perl extensions
urxvt.perl-ext-common: default,matcher,font-size
!! Open urls in browser with Control-Click
urxvt.urlLauncher: /usr/bin/firefox
urxvt.matcher.button: C1
!! Change font size on Control-Plus/Minus
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease
