*foreground:        #e0dfe2
*background:        #0B121B
*.foreground:       #e0dfe2
*.background:       #0B121B
emacs*foreground:   #e0dfe2
emacs*background:   #0B121B
URxvt*foreground:   #e0dfe2
XTerm*foreground:   #e0dfe2
UXTerm*foreground:  #e0dfe2
URxvt*background:   [100]#0B121B
XTerm*background:   #0B121B
UXTerm*background:  #0B121B
URxvt*cursorColor:  #e0dfe2
XTerm*cursorColor:  #e0dfe2
UXTerm*cursorColor: #e0dfe2
URxvt*borderColor:  [100]#0B121B
URxvt.scrollBar: false
URxvt.internalBorder:  17
URxvt.geometry: 66x16
URxvt.font: xft:Iosevka Medium:size=9.5

! clipboard
! update clipboard whenever primary gets updated
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped


